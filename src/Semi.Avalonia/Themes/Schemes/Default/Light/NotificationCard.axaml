<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="NotificationCardForeground" Color="#1C1F23" />
    <SolidColorBrush x:Key="NotificationCardBorderBrush" Opacity="0.08" Color="#1C1F23" />
    <SolidColorBrush x:Key="NotificationCardBackground" Color="White" />
    <SolidColorBrush x:Key="NotificationCardInformationIconForeground" Color="{StaticResource SemiBlue5Color}" />
    <SolidColorBrush x:Key="NotificationCardSuccessIconForeground" Color="{StaticResource SemiGreen5Color}" />
    <SolidColorBrush x:Key="NotificationCardWarningIconForeground" Color="{StaticResource SemiOrange5Color}" />
    <SolidColorBrush x:Key="NotificationCardErrorIconForeground" Color="{StaticResource SemiRed5Color}" />
    <BoxShadows x:Key="NotificationCardBoxShadows">0 0 8 0 #1A000000</BoxShadows>
</ResourceDictionary>